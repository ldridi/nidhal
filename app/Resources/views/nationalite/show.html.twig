{% extends 'base.html.twig' %}

{% block body %}
    <h1>Nationalite</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ nationalite.id }}</td>
            </tr>
            <tr>
                <th>Nomnationalite</th>
                <td>{{ nationalite.nomNationalite }}</td>
            </tr>
            <tr>
                <th>Statutnationalite</th>
                <td>{{ nationalite.statutNationalite }}</td>
            </tr>
            <tr>
                <th>Datenationalite</th>
                <td>{% if nationalite.dateNationalite %}{{ nationalite.dateNationalite|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('nationalite_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('nationalite_edit', { 'id': nationalite.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
